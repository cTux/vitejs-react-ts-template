{
  "name": "@ctux/vitejs-react-ts-template",
  "version": "1.1.0",
  "homepage": "https://github.com/cTux/vitejs-react-ts-template",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "build:storybook": "build-storybook",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "lint": "eslint .",
    "prepare": "husky install",
    "preview": "vite preview",
    "serve": "vite serve",
    "storybook": "start-storybook -p 6006",
    "template:check:dependencies": "pnpm exec depcheck",
    "template:branches:show": "pnpm exec git-removed-branches",
    "template:branches:delete": "pnpm exec git-removed-branches --prune",
    "test": "pnpm run test:vitest && pnpm run test:playwright",
    "test:playwright": "pnpm exec playwright test playwright",
    "test:vitest": "vitest --run",
    "test:vitest:coverage": "vitest --run --coverage",
    "test:vitest:watch": "vitest"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@playwright/test": "^1.28.1",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^16.18.9",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.25.8",
    "babel-loader": "^8.3.0",
    "c8": "^7.12.0",
    "concurrently": "^7.6.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.8",
    "gh-pages": "^4.0.0",
    "git-removed-branches": "^2.0.1",
    "happy-dom": "^8.1.0",
    "husky": "^8.0.2",
    "node-fetch": "^3.3.0",
    "npm-check-updates": "^16.6.0",
    "prettier": "^2.8.1",
    "sass": "^1.56.2",
    "typescript": "^4.9.4",
    "update-template": "^1.3.1",
    "vite": "^4.0.1",
    "vite-plugin-pwa": "^0.14.0",
    "vitest": "^0.25.8"
  },
  "pnpm": {
    "overrides": {
      "glob-parent@<5.1.2": ">=5.1.2",
      "async@<2.6.4": ">=2.6.4",
      "trim@<0.0.3": ">=0.0.3",
      "trim-newlines@<3.0.1": ">=3.0.1",
      "ejs@<3.1.7": ">=3.1.7",
      "async@>=2.0.0 <2.6.4": ">=2.6.4",
      "got@<11.8.5": ">=11.8.5",
      "terser@>=5.0.0 <5.14.2": ">=5.14.2",
      "minimatch@<3.0.5": ">=3.0.5"
    }
  }
}
