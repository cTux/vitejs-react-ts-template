{
  "name": "@ctux/vitejs-react-ts-template",
  "version": "0.4.0",
  "homepage": "https://github.com/cTux/vitejs-react-ts-template",
  "license": "MIT",
  "scripts": {
    "build": "vite build | gnomon",
    "deploy": "gh-pages -d dist | gnomon",
    "dev": "vite",
    "lint": "eslint . | gnomon",
    "prepare": "husky install",
    "preview": "vite preview --port 3093",
    "serve": "vite serve",
    "template:check:dependencies": "pnpm exec depcheck",
    "template:branches:show": "pnpm exec git-removed-branches",
    "template:branches:delete": "pnpm exec git-removed-branches --prune",
    "test": "pnpm run test:vitest && pnpm run test:playwright",
    "test:playwright": "pnpm exec playwright test playwright | gnomon",
    "test:vitest": "vitest --run | gnomon",
    "test:vitest:coverage": "vitest --run --coverage | gnomon",
    "test:vitest:watch": "vitest"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@playwright/test": "^1.24.0",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^16.11.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-basic-ssl": "^0.1.1",
    "@vitejs/plugin-react": "^2.0.0",
    "c8": "^7.12.0",
    "concurrently": "^7.3.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^4.0.0",
    "git-removed-branches": "^2.0.1",
    "gnomon": "^1.5.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "node-fetch": "^3.2.9",
    "npm-check-updates": "16.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.0",
    "typescript": "^4.7.4",
    "update-template": "^1.3.1",
    "vite": "^3.0.2",
    "vite-plugin-pwa": "^0.12.3",
    "vitest": "^0.19.0"
  },
  "pnpm": {
    "overrides": {
      "glob-parent@<5.1.2": ">=5.1.2",
      "async@<2.6.4": ">=2.6.4",
      "trim@<0.0.3": ">=0.0.3",
      "trim-newlines@<3.0.1": ">=3.0.1",
      "ejs@<3.1.7": ">=3.1.7",
      "async@>=2.0.0 <2.6.4": ">=2.6.4",
      "got@<11.8.5": ">=11.8.5",
      "terser@>=5.0.0 <5.14.2": ">=5.14.2"
    }
  }
}
