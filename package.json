{
  "name": "@ctux/vitejs-react-ts-template",
  "version": "1.2.0",
  "homepage": "https://github.com/cTux/vitejs-react-ts-template",
  "license": "MIT",
  "engines": {
    "node": "^18"
  },
  "scripts": {
    "build": "vite build",
    "build:storybook": "storybook build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "dev:storybook": "storybook dev -p 6006",
    "lint": "eslint .",
    "preinstall": "check-engine",
    "prepare": "playwright install chromium && husky install",
    "preview": "vite preview",
    "serve": "vite serve",
    "template:check:dependencies": "pnpm exec depcheck",
    "template:branches:show": "pnpm exec git-removed-branches",
    "template:branches:delete": "pnpm exec git-removed-branches --prune",
    "template:add:remote": "sh ./scripts/add-template-remote.sh",
    "template:update": "sh ./scripts/merge-template-changes.sh",
    "test": "pnpm run test:vitest && pnpm run test:playwright",
    "test:playwright": "pnpm exec playwright test playwright",
    "test:vitest": "vitest --run",
    "test:vitest:coverage": "vitest --run --coverage",
    "test:vitest:watch": "vitest"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "express": "^4.18.2",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.0",
    "@commitlint/config-conventional": "^17.7.0",
    "@julr/vite-plugin-validate-env": "^0.2.4",
    "@playwright/test": "^1.36.2",
    "@storybook/addon-actions": "^7.2.1",
    "@storybook/addon-essentials": "^7.2.1",
    "@storybook/addon-interactions": "^7.2.1",
    "@storybook/addon-links": "^7.2.1",
    "@storybook/react": "^7.2.1",
    "@storybook/react-vite": "^7.2.1",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.17.1",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-c8": "^0.33.0",
    "check-engine": "^1.11.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "gh-pages": "^5.0.0",
    "git-removed-branches": "^2.1.0",
    "happy-dom": "^10.9.0",
    "husky": "^8.0.3",
    "node-fetch": "^3.3.2",
    "npm-check-updates": "^16.10.18",
    "prettier": "^3.0.1",
    "sass": "^1.64.2",
    "storybook": "^7.2.1",
    "typescript": "^5.1.6",
    "update-template": "^1.3.1",
    "vite": "^4.4.9",
    "vite-plugin-chunk-split": "^0.4.7",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^0.34.1"
  },
  "pnpm": {
    "overrides": {
      "glob-parent@<5.1.2": ">=5.1.2",
      "async@<2.6.4": ">=2.6.4",
      "trim@<0.0.3": ">=0.0.3",
      "trim-newlines@<3.0.1": ">=3.0.1",
      "ejs@<3.1.7": ">=3.1.7",
      "async@>=2.0.0 <2.6.4": ">=2.6.4",
      "got@<11.8.5": ">=11.8.5",
      "terser@>=5.0.0 <5.14.2": ">=5.14.2",
      "minimatch@<3.0.5": ">=3.0.5",
      "json5@>=2.0.0 <2.2.2": ">=2.2.2",
      "http-cache-semantics@<4.1.1": ">=4.1.1",
      "cacheable-request@<10.2.7": ">=10.2.7",
      "semver@<7.5.2": ">=7.5.2",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "word-wrap@<1.2.4": ">=1.2.4"
    }
  }
}
