{
  "name": "@ctux/vitejs-react-ts-template",
  "version": "0.4.0",
  "homepage": "https://github.com/cTux/vitejs-react-ts-template",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "lint": "eslint .",
    "prepare": "husky install",
    "preview": "vite preview",
    "template:update": "update-template https://github.com/cTux/vitejs-react-ts-template",
    "template:check:dependencies": "pnpm exec depcheck",
    "template:branches:show": "git-removed-branches",
    "template:branches:delete": "git-removed-branches --prune",
    "test": "pnpm run test:vitest",
    "test:vitest": "vitest --run --globals --dom",
    "test:vitest:e2e": "nightwatch ./src/**/*.nw.js --env firefox",
    "test:vitest:coverage": "vitest --run --coverage --globals --dom",
    "test:vitest:watch": "vitest --globals --dom",
    "test:watch": "pnpm run test:vitest:watch",
    "webstorm-integration": "vitest --globals --dom --watch --reporter=dot --reporter=json --outputFile=.vitest-result.json"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.14.0",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.4",
    "@types/nightwatch": "^2.0.9",
    "@types/node": "^16.11.43",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/sinon": "^10.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "^1.3.2",
    "c8": "^7.11.3",
    "chromedriver": "^103.0.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "geckodriver": "^3.0.2",
    "gh-pages": "^4.0.0",
    "git-removed-branches": "^2.0.1",
    "happy-dom": "^6.0.2",
    "husky": "^8.0.1",
    "nightwatch": "^2.2.2",
    "node-fetch": "^3.2.6",
    "npm-check-updates": "15.2.0",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "typescript": "^4.7.4",
    "update-template": "^1.3.1",
    "vite": "^2.9.13",
    "vite-plugin-nightwatch": "^0.2.12",
    "vite-plugin-pwa": "^0.12.2",
    "vitest": "^0.17.0"
  },
  "pnpm": {
    "overrides": {
      "glob-parent@<5.1.2": ">=5.1.2",
      "async@<2.6.4": ">=2.6.4",
      "trim@<0.0.3": ">=0.0.3",
      "trim-newlines@<3.0.1": ">=3.0.1",
      "ejs@<3.1.7": ">=3.1.7",
      "async@>=2.0.0 <2.6.4": ">=2.6.4",
      "got@<11.8.5": ">=11.8.5"
    }
  }
}
