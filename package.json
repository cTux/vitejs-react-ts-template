{
  "name": "@ctux/vitejs-react-ts-template",
  "version": "1.1.1",
  "homepage": "https://github.com/cTux/vitejs-react-ts-template",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "build:storybook": "storybook build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "dev:storybook": "storybook dev -p 6006",
    "lint": "eslint .",
    "prepare": "husky install",
    "preview": "vite preview",
    "serve": "vite serve",
    "template:check:dependencies": "pnpm exec depcheck",
    "template:branches:show": "pnpm exec git-removed-branches",
    "template:branches:delete": "pnpm exec git-removed-branches --prune",
    "test": "pnpm run test:vitest && pnpm run test:playwright",
    "test:playwright": "pnpm exec playwright test playwright",
    "test:vitest": "vitest --run",
    "test:vitest:coverage": "vitest --run --coverage",
    "test:vitest:watch": "vitest"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@playwright/test": "^1.29.2",
    "@storybook/addon-actions": "^7.0.0-beta.30",
    "@storybook/addon-essentials": "^7.0.0-beta.30",
    "@storybook/addon-interactions": "^7.0.0-beta.30",
    "@storybook/addon-links": "^7.0.0-beta.30",
    "@storybook/react": "^7.0.0-beta.30",
    "@storybook/react-vite": "^7.0.0-beta.30",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-c8": "^0.27.2",
    "depcheck": "^1.4.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^4.0.0",
    "git-removed-branches": "^2.0.1",
    "happy-dom": "^8.1.4",
    "husky": "^8.0.3",
    "node-fetch": "^3.3.0",
    "npm-check-updates": "^16.6.2",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "storybook": "^7.0.0-beta.30",
    "typescript": "^4.9.4",
    "update-template": "^1.3.1",
    "vite": "^4.0.4",
    "vitest": "^0.27.2"
  },
  "pnpm": {
    "overrides": {
      "glob-parent@<5.1.2": ">=5.1.2",
      "async@<2.6.4": ">=2.6.4",
      "trim@<0.0.3": ">=0.0.3",
      "trim-newlines@<3.0.1": ">=3.0.1",
      "ejs@<3.1.7": ">=3.1.7",
      "async@>=2.0.0 <2.6.4": ">=2.6.4",
      "got@<11.8.5": ">=11.8.5",
      "terser@>=5.0.0 <5.14.2": ">=5.14.2",
      "minimatch@<3.0.5": ">=3.0.5",
      "json5@>=2.0.0 <2.2.2": ">=2.2.2"
    }
  }
}
